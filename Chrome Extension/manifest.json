{
  "manifest_version": 2,
  "name": "Maine Schedule",
  "version": "0.0.0.4",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["assets/js/main.js", "assets/js/minutes.js"]
    }
  ],
  "browser_action": {
    "default_icon": "ms.png",
    "default_popup": "popup.html",
    "default_title": "Maine Schedule"
  }
}
